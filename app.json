{
  "expo": {
    "name": "newsdata-app",
    "slug": "newsdata-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/newsdata/1024x1024.png",
    "scheme": "newsdataapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/newsdata/1024x1024.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rockie2695.newsdataapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/newsdata/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/newsdata/1024x1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6e23b164-7e95-4f5c-9522-0da87119d95e"
      }
    },
    "owner": "rockie2695"
  }
}
