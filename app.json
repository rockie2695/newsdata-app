{
  "expo": {
    "name": "newsdata-app",
    "slug": "newsdata-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/newsdata/1024x1024.png",
    "scheme": "newsdataapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/newsdata/1024x1024.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rockie2695.newsdataapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/newsdata/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/newsdata/1024x1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/noto-sans-hk/400Regular/NotoSansHK_400Regular.ttf",
            "node_modules/@expo-google-fonts/noto-sans-hk/700Bold/NotoSansHK_700Bold.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "NotoSansHK",
                "fontDefinitions": [
                  {
                    "path": "node_modules/@expo-google-fonts/noto-sans-hk/400Regular/NotoSansHK_400Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "node_modules/@expo-google-fonts/noto-sans-hk/700Bold/NotoSansHK_700Bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6e23b164-7e95-4f5c-9522-0da87119d95e"
      }
    },
    "owner": "rockie2695",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
